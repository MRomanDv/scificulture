            <div class="work-imgs">
                <div class="img-box">
                    <img src="/static/imgs/we1.jpeg">
                </div>
                <div class="img-box">
                    <img src="/static/imgs/we2.png">
                </div>
                <div class="img-box">
                    <img src="/static/imgs/we3.jpeg">
                </div>
                <div class="img-box">
                    <img src="/static/imgs/we4.jpeg">
                </div>

            </div>

        </div>
    </section>

    <section class="films">
        <div class="scfifl-title"><h2>ICONIC <span class="blue">SCI FI</span> <span class="red">FILMS</span></h2></div>
        <div class="films-posters_container">
            <div class="poster" data-director="Directed by Lilly Wachowski and Lana Wachowski">
                <img src="/static/imgs/mv1.jpeg">
            </div>
            <div class="poster" data-director="Directed by Stanley Kubrick">
                <img src="/static/imgs/mv2.jpeg">
